{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/auth/(.*)",
        "dest": "/auth.js"
      },
      {
        "src": "/webhook/(.*)",
        "dest": "/webhook.js"
      },
      {
        "src": "/(.*)",
        "dest": "/public/index.html"
      }
    ]
  }
  